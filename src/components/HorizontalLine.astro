---
interface Props {
	text?: string;
}

const { text = "<>" } = Astro.props;
---
<div class="relative flex py-5 items-center">
    <div class="grow border-t border-gray-400"></div>
    <span class="shrink mx-4 text-gray-400"> {text} </span>
    <div class="grow border-t border-gray-400"></div>
</div>
